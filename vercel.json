{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/admin", "dest": "/public/admin.html" },
    { "src": "/rastreamento", "dest": "/public/rastreamento.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
